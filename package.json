{
	"name": "gebna.gg",
	"private": true,
	"version": "0.9.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build && pagefind --site \".svelte-kit/output/prerendered/pages\"",
		"preview": "vite preview && wrangler dev",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"convert": "node --experimental-strip-types ./scripts/convert-hero.ts",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
	},
	"devDependencies": {
		"@eslint/compat": "^1.4.0",
		"@eslint/js": "^9.38.0",
		"@hpcc-js/wasm": "^2.28.0",
		"@ianvs/prettier-plugin-sort-imports": "^4.7.0",
		"@lucide/svelte": "^0.511.0",
		"@rehype-pretty/transformers": "^0.13.2",
		"@sveltejs/adapter-cloudflare": "^7.2.4",
		"@sveltejs/kit": "^2.47.3",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.1.16",
		"daisyui": "^5.3.10",
		"eslint": "^9.38.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.5",
		"fast-xml-parser": "^5.3.0",
		"globals": "^16.4.0",
		"gray-matter": "^4.0.3",
		"pagefind": "^1.4.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-pretty-code": "^0.14.1",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"sharp": "^0.34.4",
		"shiki": "^3.14.0",
		"simple-icons": "^14.15.0",
		"svelte": "^5.42.2",
		"svelte-check": "^4.3.3",
		"tailwindcss": "^4.1.16",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.46.2",
		"unified": "^11.0.5",
		"unist-util-visit": "^5.0.0",
		"valibot": "^1.1.0",
		"vite": "^6.4.1",
		"vite-plugin-pagefind": "^1.0.7",
		"wrangler": "^4.45.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
